@using WebMaze.Models.HealthDepartment

@model List<ListRecordFormViewModel>

<h1> Список всех заявок </h1>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(m => m[0].Id)
        </th>
        <th>
            @Html.DisplayNameFor(m => m[0].Name)
        </th>
        <th>
            @Html.DisplayNameFor(m => m[0].LastName)
        </th>
        <th>
            @Html.DisplayNameFor(m => m[0].PhoneNumber)
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelitem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelitem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelitem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelitem => item.PhoneNumber)
            </td>
        </tr>
    }
</table>

